{
  "name": "raidshadowapp",
  "version": "1.0.0",
  "description": "Aplicaci√≥n de escritorio para administrar los jugadores de Raid Shadow Legends",
  "main": "./out/main/index.js",
  "author": {
    "name": "Luciano Montalvan",
    "url": "https://github.com/LucianoOPM"
  },
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -m dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config ./electron-builder.yml",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "binaries": "node-gyp-build"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@material-tailwind/react": "^2.1.5",
    "@nextui-org/react": "^2.2.9",
    "@types/react": "^18.2.43",
    "electron-updater": "^6.1.1",
    "framer-motion": "^10.16.16",
    "mysql2": "^3.9.1",
    "react-router-dom": "^6.20.1",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "sweetalert2": "^11.10.1",
    "sweetalert2-react-content": "^5.0.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.3",
    "@types/react-dom": "^18.2.7",
    "@types/validator": "^13.11.8",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.32",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
